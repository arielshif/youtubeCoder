{
  "manifest_version": 3,
  "name": "YouTube Event Logger",
  "version": "1.0",
  "description": "Log events with timestamps from YouTube videos to Google Sheets",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["https://www.youtube.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
